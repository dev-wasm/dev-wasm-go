{
    "name": "Go WASM",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },

    "features": {
        "devwasm.azurecr.io/dev-wasm/dev-wasm-feature/wasmtime-wasi:0.0.9": {
            "version": "20",
            "wasmtime_version": "v15.0.1",
            "wit_version": "0.16.0"
        },
        "ghcr.io/devcontainers/features/common-utils": {}
    },
    "remoteEnv": {
        "PATH": "${containerEnv:PATH}:/usr/local/lib/wasi-sdk-20.0/bin:/usr/local/lib:/usr/local/go/bin",
        "GOROOT": "/usr/local/go"
    }
}
